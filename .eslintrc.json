{
  "env": {
    "browser": true,
    "node": true,
    "worker": false,
    "amd": false,
    "mocha": false,
    "jasmine": false,
    "phantomjs": false,
    "jquery": false,
    "prototypejs": false,
    "shelljs": false,
    "meteor": false,
    "mongo": false,
    "applescript": false,
    "es6": true
  },

  "globals": {
    "goog": true,
    "angular": false,
    "Bloodhound": false,
    "$": false,
    "jQuery": false
  },

  "plugins": [
  ],

  "rules": {
    "no-extra-semi": 1,
    "no-inner-declarations": 2,
    "no-func-assign": 0,
    "no-delete-var": 0,
    "no-multi-str": 0,
    "no-unused-vars": 0,
    "one-var": [2, "never"],
    "quotes": [2, "single"],
    "curly": 2,
    "no-eval": 2,
    "no-extend-native": 2,
    "no-new-wrappers": 2,
    "no-with": 2,
    "no-undef": 2,
    "array-bracket-spacing": [2, "never"],
    "indent": [2, 2],
    "no-array-constructor": 2,
    "no-mixed-spaces-and-tabs": 2,
    "no-new-object": 2,
    "object-curly-spacing": [2, "never"],
    "semi": 2,
    "max-len": [1, 80, 2]
  }
}
