extends index

block content

  article.object
    .container-fluid
      .row
        .col-md-6
          .row
            .col-md-6
              img.object__image(src="https://placekitten.com/g/400/400")
            .col-md-6
              img.object__image(src="https://placekitten.com/g/400/400")
            .col-md-6
              img.object__image(src="https://placekitten.com/g/400/400")
            .col-md-6
              img.object__image(src="https://placekitten.com/g/400/400")
            each asset in metadata.relatedAssets
              .col-md-6= asset.assetID
        .col-md-6
          h1= metadata.workDescription || 'Ingen arbejdsbetegnelse'
    .container-fluid
      .row
        .col-sm-6
          != objectSection('place', metadata)
