if player.images.length === 1
  - const image = player.images[0];
  img(src=image.thumbnail,
      alt=image.title,
      data-action='toggle-expandable').asset__image
else
  .row
    each image in player.images
      .col-md-4
        img(src=image.thumbnail,
            alt=image.title,
            data-action='toggle-expandable').asset__image
