{
  "sections": {
    "events": {
      "title": "Hændelser",
      "rows": [
        {
          "title": "Erhvervelse",
          "template": "= (accessionEvents || []).map(formatEvent).join(', ')"
        },
        {
          "title": "Fremstilling",
          "template": "= (creationEvents || []).map(formatEvent).join(', ')"
        },
        {
          "title": "Fund",
          "template": "= (foundEvents || []).map(formatEvent).join(', ')"
        }
      ]
    },
    "objectinfo": {
      "title": "Genstandsoplysninger",
      "rows": [
        {
          "title": "Materiale",
          "template": "= materials && materials.map(m => m.material).join(', ')"
        },
        {
          "type": "title-and-value-list",
          "field": "measurements",
          "templates": {
            "title": "= dimension + ':'",
            "value": "= (data && decimals(data, 1)) + ' ' + (unit || '')"
          }
        },
        {
          "title": "Nation",
          "template": "= nation"
        },
        {
          "title": "Møntsted",
          "template": "= place"
        },
        {
          "title": "Autoritet",
          "template": "= authority"
        },
        {
          "title": "Nominal",
          "template": "= nominal"
        }
      ]
    },
    "place": {
      "title": "Sted",
      "rows": [
        {
          "title": "Fremstillet",
          "template": "= (creationEvents || []).map(e => e.place).join(', ')"
        }, {
          "title": "Brugt",
          "template": "= (usageEvents || []).map(e => e.place).join(', ')"
        }, {
          "title": "Fundet",
          "template": "= (foundEvents || []).map(e => e.place).join(', ')"
        }, {
          "title": "Indsamlet",
          "template": "= (accessionEvents || []).map(e => e.place).join(', ')"
        }
      ]
    }
  }
}
