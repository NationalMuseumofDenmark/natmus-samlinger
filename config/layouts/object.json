{
  "sections": {
    "events": {
      "title": "Hændelser",
      "rows": [
        {
          "title": "Erhvervelse",
          "template": "= (accessionEvents || []).map(formatEvent).join(', ')"
        },
        {
          "title": "Fremstilling",
          "template": "= (creationEvents || []).map(formatEvent).join(', ')"
        },
        {
          "title": "Fund",
          "template": "= (foundEvents || []).map(formatEvent).join(', ')"
        }
      ]
    },
    "objectinfo": {
      "title": "Genstandsoplysninger",
      "rows": [
        {
          "title": "Materiale",
          "template": "= materials && materials.map(m => m.material).join(', ')"
        },
        {
          "type": "title-and-value-list",
          "field": "measurements",
          "templates": {
            "title": "= dimension + ':'",
            "value": "= (data && decimals(data, 1)) + ' ' + (unit || '')"
          }
        },
        {
          "title": "Nation",
          "template": "= nation"
        },
        {
          "title": "Møntsted",
          "template": "= place"
        },
        {
          "title": "Autoritet",
          "template": "= authority"
        },
        {
          "title": "Nominal",
          "template": "= nominal"
        }
      ]
    },
    "techinfo": {
      "title": "Tekniske oplysninger",
      "rows": [
        {
          "title": "Post oprettelsesdato",
          "template": "= createdDate && formatDate(createdDate)"
        },
        {
          "title": "Genstandsnummer",
          "template": "= objectIdentification"
        },
        {
          "title": "System-ID",
          "template": "= id"
        }
      ]
    },
    "place": {
      "title": "Sted",
      "rows": [
        {
          "title": "Fremstillet",
          "template": "= (creationEvents || []).map(e => e.place).join(', ')"
        }, {
          "title": "Brugt",
          "template": "= (usageEvents || []).map(e => e.place).join(', ')"
        }, {
          "title": "Fundet",
          "template": "= (foundEvents || []).map(e => e.place).join(', ')"
        }, {
          "title": "Indsamlet",
          "template": "= (accessionEvents || []).map(e => e.place).join(', ')"
        }
      ]
    },
    "time": {
      "title": "Tid",
      "rows": [
        {
          "title": "Fremstillingstidspunkt",
          "type": "date-interval",
          "fields": {
            "from": "creationEvents[0].yearFrom",
            "to": "creationEvents[0].yearTo"
          }
        },
        {
          "title": "Fremstillingstidspunkt",
          "type": "date-interval",
          "fields": {
            "from": "creationEvents[0].dateFrom",
            "to": "creationEvents[0].dateTo"
          }
        },
        {
          "title": "Brugstidspunkt",
          "type": "date-interval",
          "fields": {
            "from": "usageEvents[0].yearFrom",
            "to": "usageEvents[0].yearTo"
          }
        },
        {
          "title": "Brugstidspunkt",
          "type": "date-interval",
          "fields": {
            "from": "usageEvents[0].dateFrom",
            "to": "usageEvents[0].dateTo"
          }
        },
        {
          "title": "Fundstidspunkt",
          "type": "date-interval",
          "fields": {
            "from": "foundEvents[0].yearFrom",
            "to": "foundEvents[0].yearTo"
          }
        },
        {
          "title": "Fundstidspunkt",
          "type": "date-interval",
          "fields": {
            "from": "foundEvents[0].dateFrom",
            "to": "foundEvents[0].dateTo"
          }
        },
        {
          "title": "Indsamlingstidspunkt",
          "type": "date-interval",
          "fields": {
            "from": "accessionEvents[0].yearFrom",
            "to": "accessionEvents[0].yearTo"
          }
        },
        {
          "title": "Indsamlingstidspunkt",
          "type": "date-interval",
          "fields": {
            "from": "accessionEvents[0].dateFrom",
            "to": "accessionEvents[0].dateTo"
          }
        }
      ]
    }
  }
}
