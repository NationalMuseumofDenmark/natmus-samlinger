{
  "sections": {
    "events": {
      "title": "Hændelser",
      "rows": [
        {
          "title": "Erhvervelse",
          "template": "= (accessionEvents || []).map(formatEvent).join(', ')"
        },
        {
          "title": "Fremstilling",
          "template": "= (creationEvents || []).map(formatEvent).join(', ')"
        },
        {
          "title": "Fund",
          "template": "= (foundEvents || []).map(formatEvent).join(', ')"
        }
      ]
    },
    "objectinfo": {
      "title": "Genstandsoplysniner",
      "rows": [
        {
          "title": "Materiale",
          "template": "= materials && materials.map(m => m.material).join(', ')"
        },
        {
          "type": "title-and-value-list",
          "field": "measurements",
          "templates": {
            "title": "= dimension + ':'",
            "value": "= (data && decimals(data, 1)) + ' ' + (unit || '')"
          }
        },
        {
          "title": "Nation",
          "template": "= nation"
        },
        {
          "title": "Møntsted",
          "template": "= place"
        },
        {
          "title": "Autoritet",
          "template": "= authority"
        },
        {
          "title": "Nominal",
          "template": "= nominal"
        }
      ]
    },
    "techinfo": {
      "title": "Tekniske oplysninger",
      "rows": [
        {
          "title": "Post oprettelsesdato",
          "template": "= createdDate && formatDate(createdDate)"
        },
        {
          "title": "Genstandsnummer",
          "template": "= objectIdentification"
        },
        {
          "title": "System-ID",
          "template": "= id"
        }
      ]
    }
  }
}
