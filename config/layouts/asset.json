{
  "sections": {
    "place": {
      "title": "Sted",
      "rows": [
        {
          "title": "Adresse",
          "template": "= address || street"
        }, {
          "title": "By",
          "template": "= city"
        }, {
          "title": "Topnr",
          "template": "= topno"
        }, {
          "title": "Koordinater",
          "type": "map-coordinates",
          "fields": {
            "latitude": "latitude",
            "longitude": "longitude"
          }
        }
      ]
    },
    "time": {
      "title": "Tid",
      "rows": [
        {
          "title": "Optagelsestid",
          "type": "date-interval",
          "fields": {
            "from": "date.recorded.from",
            "to": "date.recorded.to"
          }
        }, {
          "title": "Fremstillingstid",
          "type": "date-interval",
          "fields": {
            "from": "date.created.from",
            "to": "date.created.to"
          }
        }, {
          "title": "Tidsnote",
          "template": "= timenote"
        }
      ]
    },
    "additional-information": {
      "title": "Yderligere oplysninger",
      "rows": [
        {
          "title": "Samling",
          "template": "!= catalogLinked(catalogs, catalog)"
        }, {
          "title": "Licens",
          "template": "!= licenseLinked(license)"
        }, {
          "title": "Fotograf/skaber",
          "template": "= creators(actor)"
        }, {
          "title": "Kilde",
          "template": "= rights.administrator"
        }, {
          "title": "Arkivalie nr",
          "template": "= archiveno"
        }, {
          "title": "Negativ nr",
          "template": "= negative_number"
        }, {
          "title": "Genstands nr",
          "template": "= object_number"
        }, {
          "title": "Inventar nr",
          "template": "= inventory_number"
        }, {
          "title": "Arkivnavn",
          "template": "= archivename ? archivename.displaystring : ''"
        }
      ]
    },
    "technical-information": {
      "title": "Tekniske oplysninger",
      "rows": [
        {
          "title": "Fil-ID",
          "template": "= id"
        }, {
          "title": "Filstørrelse (mb)",
          "template": "= natmusFilesizeMB(file.size)"
        }, {
          "title": "Størrelse (pixel)",
          "template": "!= decimals(width_px, 0) + ' &times; ' + decimals(height_px, 0)"
        }, {
          "title": "Størrelse (cm)",
          "template": "!= decimals(width_cm, 1) + ' &times; ' + decimals(height_cm, 1)"
        }, {
          "title": "Opløsning (DPI)",
          "template": "= decimals(file.dimensions.ppi, 0)"
        }
      ]
    }
  }
}
