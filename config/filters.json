{
  "collection": {
    "field": "collection.keyword",
    "plural": "samlinger",
    "singular": "samling",
    "translations": "collections",
    "type": "term"
  },
  "creation": {
    "fields": {
      "from": "date.created.from.timestamp",
      "to": "date.created.to.timestamp"
    },
    "plural": "fremstillingstider",
    "singular": "fremstillingstid",
    "type": "date-interval-range"
  },
  "media": {
    "field": "file.mediaType.keyword",
    "plural": "medietyper",
    "singular": "medietype",
    "translations": "mediaFileTypes",
    "type": "term"
  },
  "license": {
    "field": "rights.license.keyword",
    "plural": "licenser",
    "singular": "licens",
    "type": "term"
  },
  "size": {
    "filters" : {
      "Minimum 1000px": {
        "bool": {
          "should": [
            {"range": { "file.dimensions.width": { "gte": 1000 }}},
            {"range": { "file.dimensions.height": { "gte": 1000 }}}
          ]
        }
      },
      "Minimum 2500px": {
        "bool": {
          "should": [
            {"range": { "file.dimensions.width": { "gte": 2500 }}},
            {"range": { "file.dimensions.height": { "gte": 2500 }}}
          ]
        }
      },
      "Minimum 4000px": {
        "bool": {
          "should": [
            {"range": { "file.dimensions.width": { "gte": 4000 }}},
            {"range": { "file.dimensions.height": { "gte": 4000 }}}
          ]
        }
      },
      "Printvenling": {
        "bool": {
          "should": [
            {"range": { "file.dimensions.ppi": { "gte": 300 }}},
            {"range": { "file.dimensions.width": { "gte": 2400 }}},
            {"range": { "file.dimensions.height": { "gte": 2400 }}}
          ]
        }
      }
    },
    "plural": "størrelser",
    "singular": "størrelse",
    "type": "filters"
  },
  "location": {
    "filters" : {
      "Har placering": {
        "bool": {
          "should": [
            {"exists": { "field": "location.verified.latitude" }},
            {"exists": { "field": "location.crowd.latitude" }}
          ]
        }
      },
      "Mangler placering": {
        "bool": {
          "must_not": [
            {"exists": { "field": "location.verified.latitude" }},
            {"exists": { "field": "location.crowd.latitude" }}
          ]
        }
      }
    },
    "plural": "placeringer",
    "singular": "placering",
    "type": "filters"
  },
  "q": {
    "skipSplit": true,
    "type": "querystring"
  }
}
